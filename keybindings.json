[
    {
        "key": "shift+cmd+n",
        "command": "workbench.action.files.newUntitledFile"
    },
    {
        "key": "cmd+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "cmd+n",
        "command": "workbench.action.newWindow"
    },
    {
        "key": "shift+cmd+n",
        "command": "-workbench.action.newWindow"
    },
    { 
        "key": "alt+b",
        "command": "cursorWordStartLeft"
    },
    { 
