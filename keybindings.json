// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "alt+b",
    "command": "cursorWordStartLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+f",
    "command": "cursorWordEndRight",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+d",
    "command": "deleteWordRight",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+r",
    "command": "revealFileInOS",
    "when": "!terminalFocus"
  },
  {
    "key": "alt+l",
    "command": "editor.action.openLink",
    "when": "!editorHasSelection && editorTextFocus && inSearchEditor || editorLangId == 'markdown'" 
  },
  {
    "key": "alt+l",
    "command": "robenkleene.openFile",
    "when": "editorTextFocus && !editorHasSelection && editorLangId != 'markdown' && !inSearchEditor"
  },
  {
    "key": "alt+t",
    "command": "workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+s",
    "command": "actions.find",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+cmd+n",
    "command": "explorer.newFile",
    "when": "!inputFocus && filesExplorerFocus"
  },
  {
    "key": "cmd+k cmd+0",
    "command": "workbench.files.action.collapseExplorerFolders",
    "when": "!inputFocus && filesExplorerFocus"
  },
  {
    "key": "shift+cmd+n",
    "command": "explorer.newFolder",
    "when": "!inputFocus && filesExplorerFocus"
  },
  {
    "key": "alt+o",
    "command": "-C_Cpp.SwitchHeaderSource",
    "when": "editorTextFocus && editorLangId == 'c'"
  },
  {
    "key": "alt+o",
    "command": "-C_Cpp.SwitchHeaderSource",
    "when": "editorTextFocus && editorLangId == 'cpp'"
  },
  {
    "key": "cmd+m",
    "command": "-markdown.extension.editing.toggleMath",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "alt+s l",
    "command": "outline.focus",
    "when": "!terminalFocus"
  },
  {
    "key": "'",
    "command": "editor.action.insertSnippet",
    "when": "editorHasSelection || editorHasMultipleSelections",
    "args": {
      "snippet": "'${TM_SELECTED_TEXT}'"
    }
  },
  {
    "key": "shift+'",
    "command": "editor.action.insertSnippet",
    "when": "editorHasSelection || editorHasMultipleSelections",
    "args": {
      "snippet": "\"${TM_SELECTED_TEXT}\""
    }
  },
  {
    "key": "ctrl+alt+w",
    "command": "editor.action.toggleWordWrap",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+x",
    "command": "workbench.action.showCommands",
    "when": "!terminalFocus"
  },
  {
    "key": "alt+[",
    "command": "spaceBlockJumper.moveUp",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+]",
    "command": "spaceBlockJumper.moveDown",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+shift+[",
    "command": "spaceBlockJumper.selectUp",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+shift+]",
    "command": "spaceBlockJumper.selectDown",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k v",
    "command": "markdown.showPreviewToSide",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+b",
    "command": "-markdown.extension.editing.toggleBold"
  },
  {
    "key": "cmd+k cmd+v",
    "command": "markdown-preview-enhanced.openPreviewToTheSide"
  },
  {
    "key": "cmd+alt+n",
    "command": "workbench.action.newWindow"
  },
  {
    "key": "`",
    "command": "editor.action.insertSnippet",
    "when": "editorHasSelection || editorHasMultipleSelections",
    "args": {
      "snippet": "`${TM_SELECTED_TEXT}`"
    }
  },
  {
    "key": "shift+alt+enter",
    "command": "markdown.extension.editing.toggleList",
    "when": "editorTextFocus && editorLangId == 'markdown'"
  },
  {
    "key": "alt+s alt+m",
    "when": "!editorTextFocus",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "shift+space",
    "when": "!editorTextFocus",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    // Codespaces doesn't have this keyboard shortcut by default
    "key": "cmd+0",
    "command": "workbench.action.focusSideBar"
  },
  {
    "key": "alt+s m",
    "when": "!editorTextFocus",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "alt+s .",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+.",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "alt+n",
    "command": "search.action.focusNextSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "alt+p",
    "command": "search.action.focusPreviousSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "alt+c",
    "command": "-markdown.extension.checkTaskList"
  },
  {
    "key": "alt+c",
    "command": "-gitlens.showQuickCommitFileDetails"
  }
]
