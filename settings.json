{
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.colorCustomizations": {
    "editor.foreground": "#aabbcc"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "punctuation.definition.heading.markdown, punctuation.definition.string.begin.markdown, punctuation.definition.string.end.markdown, punctuation.definition.metadata.markdown, markup.underline.link.markdown, punctuation.definition.bold.markdown, markup.underline.link.image.markdown, punctuation.definition.italic.markdown",
        "settings": {
          "foreground": "#777"
        }
      },
      {
        "scope": "punctuation.definition.list.begin.markdown, markup.list, text",
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "scope": "markup.raw.block.markdown, markup.inline.raw.string.markdown, markup.quote.markdown",
        "settings": {
          "foreground": "#888"
        }
      },
      {
        "scope": "entity.name.section.markdown, markup.italic.markdown, markup.bold.markdown",
        "settings": {
          "foreground": "#fff"
        }
      }
    ]
  },
  "update.showReleaseNotes": false,
  "workbench.colorTheme": "GitHub Dark",
  "workbench.activityBar.visible": true,
  "C_Cpp.updateChannel": "Insiders",
  "editor.minimap.enabled": false,
  "editor.fontFamily": "'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "spellright.spellContextByClass": {
    "markdown": "body comments strings"
  },
  "spellright.ignoreRegExpsByClass": {
    "markdown": ["/\t.*/g"]
  },
  "search.useGlobalIgnoreFiles": true,
  "debug.onTaskErrors": "showErrors",
  "editor.renderIndentGuides": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "bracket-pair-colorizer-2.excludedLanguages": [
    "markdown"
  ],
  "c-cpp-flylint.cppcheck.enable": false,
  "c-cpp-flylint.flexelint.enable": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabCompletion": "onlySnippets",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.cursorBlinking": "phase",
  "workbench.editor.closeOnFileDelete": true,
  "extensions.ignoreRecommendations": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "bracket-pair-colorizer-2.showVerticalScopeLine": false,
  "breadcrumbs.enabled": false,
  "window.zoomLevel": 0,
  "files.defaultLanguage": "markdown",
  "editor.occurrencesHighlight": false,
  "editor.showFoldingControls": "always",
  "window.openFilesInNewWindow": "default",
  "explorer.compactFolders": false,
  "editor.find.autoFindInSelection": "multiline",
  "gitlens.currentLine.enabled": false
}
